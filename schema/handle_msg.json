{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HandleMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "receive"
      ],
      "properties": {
        "receive": {
          "type": "object",
          "required": [
            "amount",
            "from",
            "sender"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint128"
            },
            "from": {
              "$ref": "#/definitions/HumanAddr"
            },
            "msg": {
              "anyOf": [
                {
                  "$ref": "#/definitions/Binary"
                },
                {
                  "type": "null"
                }
              ]
            },
            "sender": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "bet"
      ],
      "properties": {
        "bet": {
          "type": "object",
          "required": [
            "number",
            "tier"
          ],
          "properties": {
            "number": {
              "type": "integer",
              "format": "int16"
            },
            "tier": {
              "type": "integer",
              "format": "int8"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "withdrawl"
      ],
      "properties": {
        "withdrawl": {
          "type": "object",
          "required": [
            "round",
            "tier"
          ],
          "properties": {
            "round": {
              "type": "integer",
              "format": "uint32",
              "minimum": 0.0
            },
            "tier": {
              "type": "integer",
              "format": "int8"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "change_triggerer"
      ],
      "properties": {
        "change_triggerer": {
          "type": "object",
          "required": [
            "triggerer"
          ],
          "properties": {
            "triggerer": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "change_tier"
      ],
      "properties": {
        "change_tier": {
          "type": "object",
          "required": [
            "entry_fee",
            "max_rand_number",
            "min_entries",
            "tier",
            "triggerer_fee"
          ],
          "properties": {
            "entry_fee": {
              "$ref": "#/definitions/Uint128"
            },
            "max_rand_number": {
              "type": "integer",
              "format": "int16"
            },
            "min_entries": {
              "type": "integer",
              "format": "int16"
            },
            "tier": {
              "type": "integer",
              "format": "int8"
            },
            "triggerer_fee": {
              "$ref": "#/definitions/Uint128"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "trigger_lucky_number"
      ],
      "properties": {
        "trigger_lucky_number": {
          "type": "object",
          "required": [
            "entropy",
            "tier1",
            "tier2",
            "tier3"
          ],
          "properties": {
            "entropy": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "tier1": {
              "type": "boolean"
            },
            "tier2": {
              "type": "boolean"
            },
            "tier3": {
              "type": "boolean"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "Binary": {
      "description": "Binary is a wrapper around Vec<u8> to add base64 de/serialization with serde. It also adds some helper methods to help encode inline.\n\nThis is only needed as serde-json-{core,wasm} has a horrible encoding for Vec<u8>",
      "type": "string"
    },
    "HumanAddr": {
      "type": "string"
    },
    "Uint128": {
      "type": "string"
    }
  }
}
